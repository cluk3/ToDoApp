<% provide(:title, @user.name) %>
<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
      <%= render 'shared/user_info', user_info: @user %>
    </section>
  </aside>
  <div class="col-md-8">
      <h3>Created Tasks</h3>
      <%= render 'shared/created_tasks' %>
      <h3>Assigned Tasks</h3>
      <%= render 'shared/assigned_tasks' %>
  </div>
</div>